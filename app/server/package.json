    "debug": "^2.2.0",
    "dotenv": "^10.0.0",
    "express": "^4.14.0",
    "http": "0.0.1-security",
    "i18n": "^0.13.3",
    "jsdom": "^17.0.0",
    "luxon": "^1.27.0",
    "morgan": "^1.7.0",
    "natives": "^1.1.6",
    "node-fetch": "^2.6.7",
    "pdfkit": "^0.13.0",
    "pug": "^3.0.2",
    "query-string": "^7.1.0",
